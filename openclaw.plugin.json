{
  "id": "openclaw-membrane",
  "name": "@vainplex/openclaw-membrane",
  "version": "0.1.0",
  "description": "Membrane bridge for OpenClaw",
  "entry": "index.ts",
  "config": {
    "grpc_endpoint": "localhost:50051",
    "buffer_size": 1000,
    "default_sensitivity": "low"
  },
  "configSchema": {
    "type": "object",
    "properties": {
      "grpc_endpoint": {
        "type": "string",
        "default": "localhost:50051"
      },
      "buffer_size": {
        "type": "number",
        "default": 1000
      },
      "default_sensitivity": {
        "type": "string",
        "default": "low"
      }
    }
  },
  "capabilities": ["event_subscription"]
}
